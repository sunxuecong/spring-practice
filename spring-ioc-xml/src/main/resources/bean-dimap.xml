<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="teacherone" class="com.atguigu.spring6.iocxml.dimap.Teacher">
        <property name="teacherid" value="1" />
        <property name="teachername" value="张老师" />
    </bean>

    <bean id="teachertwo" class="com.atguigu.spring6.iocxml.dimap.Teacher">
        <property name="teacherid" value="3" />
        <property name="teachername" value="李老师" />
    </bean>


    <bean id="student"  class="com.atguigu.spring6.iocxml.dimap.Student">
        <property name="sid" value="no-01" />
        <property name="sname" value="史珍香" />
        <property name="teacherMap">
            <map>
                <entry>
                    <key>
                        <value>10086</value>
                    </key>
                    <ref bean="teacherone"/>
                </entry>
                <entry>
                    <key>
                        <value>10010</value>
                    </key>
                    <ref bean="teachertwo"/>
                </entry>
            </map>
        </property>
    </bean>


</beans>