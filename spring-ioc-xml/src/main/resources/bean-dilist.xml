<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


        <bean id="empone" class="com.atguigu.spring6.iocxml.ditest.Emp">
            <property name="ename" value="李白"></property>
            <property name="age" value="20"></property>
            <property name="loves">
                <array>
                    <value>吃饭</value>
                    <value>睡觉</value>
                </array>
            </property>
        </bean>

        <bean id="emptwo" class="com.atguigu.spring6.iocxml.ditest.Emp">
            <property name="ename" value="杜甫"></property>
            <property name="age" value="30"></property>
            <property name="loves">
                <array>
                    <value>写诗</value>
                    <value>写代码</value>
                </array>
            </property>
        </bean>
        <bean id="dept" class="com.atguigu.spring6.iocxml.ditest.Dept">
            <property name="dname" value="施工部"/>
            <property name="empList">
                <list>
                    <ref bean="empone"></ref>
                    <ref bean="emptwo"></ref>
                </list>
            </property>
        </bean>
</beans>